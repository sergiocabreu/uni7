<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">

	<!-- CONVERTERS -->
	<converter>
		<converter-id>entityConverter</converter-id>
		<converter-class>com.bolao.converter.EntityConverter</converter-class>
	</converter>
	<converter>
		<converter-id>dateConverter</converter-id>
		<converter-class>com.bolao.converter.DateConverter</converter-class>
	</converter>

</faces-config>
